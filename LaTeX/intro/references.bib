@inproceedings{barry_changes_2001,
	title = {Changes in cane soil properties from applications of sugarmill by-products.},
	abstract = {This paper provides information on the effects on soil chemical properties of using filter mud [filter cake] and/or boiler ash, based on 'paired sites' from 33 locations in Queensland and NSW canelands (Australia). Concentrations of nutrients and contaminant heavy metals in soils where these byproducts have been used were typically within ranges reported in other surveys of canelands. The exception was P, where high loading rates well in excess of immediate crop requirements, have raised acid extractable soil P levels (up to 1260 mg/kg). This alone is sufficient reason to change the current recycling practice both from an environmental as well as an economic viewpoint. It is estimated that it will take 4-5 applications of filter mud to raise soil total P concentrations at sites with no history of filter mud usage to comparable levels found at sites with long-term usage. A benefit from using these byproducts is an increase in soil pH levels (in some cases up to 1.5 pH units), particularly on light textured soils, with concomitant reductions in exchangeable acidity and percent aluminium saturation. While soil total cadmium loadings also increased with byproduct use, reductions in calcium chloride extractable cadmium suggest the increase in soil pH may reduce the plant available cadmium fraction. Changes in management practices affecting soil pH, and other relevant soil properties, will need to be considered particularly when other more sensitive food crops are grown on these soils.},
	language = {English},
	publisher = {PK Editorial Services Pty Ltd},
	author = {Barry, G. A. and Rayment, G. E. and Jeffrey, A. J. and Price, A. M.},
	year = {2001},
	keywords = {boiler ash, filter cake, filter mud, heavy metals, mill byproducts, soil pH, sugarcane},
	pages = {185--191},
}

