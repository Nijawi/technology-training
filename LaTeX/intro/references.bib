	note = {WOS:000315569300004},
	keywords = {Chemical pretreatment, alkaline hydrogen-peroxide, cellulose, corn stalks, enzymatic-hydrolysis, enzyme, ethanol, ethanol production, hydrolysis, saccharification, steam explosion, stover, sugarcane bagasse, wet oxidation, wheat-straw},
	pages = {125--143},
}

